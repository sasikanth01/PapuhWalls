    <!-- Changelog -->
    <!-- Initial Release -->
    <string name="v2_line1">Initial Release</string>
    <!-- Version 2.0.1 -->
    <string name="v21_line1">- Fixed FC with "clear cache" menu</string>
    <string name="v21_line2">- Fixed Muzei navbar not being tinted</string>
    <!-- Version 2.0.2 -->
    <string name="v22_line1">- Improvements to the wall fragments</string>
    <string name="v22_line2">- Remove header\'s profile selection tab</string>
    <string name="v22_line3">- 20 new wallpapers</string>
    <!-- Version 2.0.3 -->
    <string name="v23_line1">- More under the hood fixes (Thanks Andrzej)</string>
    <string name="v23_line2">- Horizontal scrolling/Pinch to zoom </string>
    <string name="v23_line3">- Pushbullet notification menu (Thanks Sasi)</string>
    <!-- Version 2.0.4 -->
    <string name="v24_line1">- Reworked how settings handle themeing (Thanks Andrzej)</string>
    <string name="v24_line2">- Added time-context headers to material drawer</string>
    <string name="v24_line3">- Added 95 new wallpapers</string>
    <string name="v24_line4">- Added French translations (Thanks Kevin)</string>
    <!-- Version 2.0.5 -->
    <string name="v25_line1">- Add more themeing functionality to settings</string>
    <string name="v25_line2">- Fixes to the way FAB works with wallpaper actions</string>
    <string name="v25_line3">- Added WIP grid changer</string>

